{
  "name": "mapeo-desktop",
  "description": "An experimental offline mapping app based on iD Editor, for indigenous territory mapping in remote environments",
  "productName": "Mapeo",
  "version": "2.1.0",
  "productDescription": "Offline Map Editor",
  "author": "Digital Democracy",
  "main": "index.js",
  "engine": {
    "node": "6.2.1"
  },
  "scripts": {
    "clean": "rimraf \"dist/Mapeo-win32-x64\"",
    "package-win": "node bin/package_windows.js",
    "installer-win": "node bin/create_windows_installer.js",
    "dmg-mac": "node bin/create_macos_dmg.js",
    "package-mac": "node bin/package_macos.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "rebuild-leveldb": "cd node_modules/leveldown && cross-env HOME=~/.electron-gyp node-gyp rebuild --target=1.7.5 --runtime=electron --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "start": "electron . --disable-http-cache",
    "server": "node index.js --headless",
    "dev": "electron . --debug --disable-http-cache",
    "postinstall": "npmpd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/mapeo-desktop.git"
  },
  "keywords": [
    "osm",
    "p2p",
    "peer-to-peer",
    "gis",
    "maps",
    "iD",
    "osm-p2p"
  ],
  "contributors": [
    "substack",
    "Gregor MacLennan <gmaclennan@digital-democracy.org>",
    "noffle"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digidem/mapeo-desktop/issues"
  },
  "homepage": "https://github.com/digidem/mapeo-desktop#readme",
  "dependencies": {
    "JSONStream": "^1.0.3",
    "application-config": "^1.0.1",
    "application-config-path": "^0.1.0",
    "body": "^5.1.0",
    "bonjour": "^3.5.0",
    "concat-stream": "^1.5.1",
    "dialogs": "^1.1.20",
    "dom-helpers": "^3.3.1",
    "ecstatic": "^2.1.0",
    "electron-debug": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.3.1",
    "end-of-stream": "^1.1.0",
    "fs-blob-store": "^5.2.1",
    "fs-copy-file-sync": "^1.0.1",
    "gtran-shapefile": "^1.1.3",
    "html-react-parser": "^0.4.6",
    "hyperlog": "^4.12.0",
    "hyperlog-index": "^5.2.2",
    "hyperlog-sneakernet-replicator": "^1.1.2",
    "hyperquest": "^2.1.3",
    "id-mapeo": "^1.0.12",
    "insert-css": "^1.0.0",
    "level": "^1.4.0",
    "levelup": "^1.3.1",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "mirror-folder": "^2.1.1",
    "mkdirp": "^0.5.1",
    "once": "^1.3.3",
    "osm-p2p": "^2.0.0",
    "osm-p2p-api": "^1.0.1",
    "osm-p2p-geojson": "^3.0.4",
    "osm-p2p-server": "^2.3.5",
    "pump": "^1.0.1",
    "randombytes": "^2.0.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "rimraf": "^2.5.2",
    "run-series": "^1.1.4",
    "semver": "^5.3.0",
    "split2": "^2.0.1",
    "statuses": "^1.3.0",
    "styled-components": "^3.2.6",
    "subleveldown": "^2.1.0",
    "tar-fs": "^1.16.0",
    "to2": "^1.0.0",
    "websocket-stream": "^3.1.0",
    "xhr": "^2.2.0",
    "xtend": "^4.0.1"
  },
  "darwinDependencies": {
    "appdmg": "0.4.5"
  },
  "devDependencies": {
    "cross-env": "^1.0.8",
    "cross-spawn": "^4.0.2",
    "devtron": "^1.2.0",
    "electron": "^2.0.2",
    "electron-packager": "^7.6.0",
    "fs-extra": "^0.30.0",
    "hubfs.js": "^1.0.0",
    "npm-platform-dependencies": "0.0.12",
    "webpack": "^3.12.0",
    "webpack-node-externals": "^1.7.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1"
  }
}
